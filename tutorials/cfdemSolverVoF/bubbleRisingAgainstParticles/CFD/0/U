FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    object      Uair;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
	walls
	{
        type            slip;
    }
	
	outlet
    {
        type            cyclic;
    }
	
    inlet
    {
       type            cyclic;
    }
}

