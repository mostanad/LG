FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      vorticity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform (0 -0 0);

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform (0 -0 0);
    }
    p1
    {
        type            cyclic;
    }
    p2
    {
        type            cyclic;
    }
}


